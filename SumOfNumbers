#include <iostream>

using namespace std;

int a,b,c;

int main(){

  cout << "This programm will give you the  sum of the range of integer numbers you provide" << endl;
  cout << "Give me the lower bound: ";
  cin >> b;
  cout << "Give me the upper bound: ";
  cin >> c ;
  a = b;

  do {
    b = (b + 1) ;
    a = (a + b) ;

  } while(b < c);
  cout << "The total sum is = "<<a<<endl;
  return 0;

}
